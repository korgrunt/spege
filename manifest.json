{
  "manifest_version": 2,
  "name":"Spg Extension",
  "description":"System de notation des url",
  "options_page":"page_options.html",
  "version": "1.0",
  "permissions":[
    "http://*/*",
    "https://*/*",
    "tabs",
    "windows"
  ],
  "browser_action":{
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons":{
    "128": "img/icon_128.png"
  },
  "background":{
    "scripts": ["components/background.js"]
  }
}
